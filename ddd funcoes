#include <iostream>
 
using namespace std;
 
int n;
string lugar;

int entrada(){
	cin >> n;
	return(n);
}

string local(){
	switch(n){
    	case 61: lugar = "Brasilia";
    	    break;
    	case 71: lugar = "Salvador";
		    break;
		case 11: lugar = "Sao Paulo";
		    break;
		case 21: lugar = "Rio de Janeiro";
		    break;
		case 32: lugar = "Juiz de Fora";
		    break;
		case 19: lugar = "Campinas";
		    break;
		case 27: lugar = "Vitoria";
		    break;
		case 31: lugar = "Belo Horizonte";
		    break;
		default: lugar = "DDD nao cadastrado";	  
	}
	return(lugar);
}

void saida(){
	cout << lugar << endl;
}

int main(){
	entrada();
	local();
	saida();
    return 0;
}
