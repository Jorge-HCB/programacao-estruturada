#include <iostream>
#include <iomanip>
 
using namespace std;
 
int main() {
 
    int N, c=0;
    double n1, n2, n3, media;
    
    cout << fixed << setprecision(1);
    
    cin >> N;
	
	for(c=0; c != N; c++){
		cin >> n1 >> n2 >> n3;
		media = (n1*2 + n2*3 + n3*5)/10;
		cout << media << endl;
	}
 
    return 0;
}
